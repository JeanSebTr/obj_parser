#include "Convert.h"
#include <sstream>

std::string Convert::itos(int i)
{
    std::ostringstream str;
    str << i;
    return str.str();
}

std::string Convert::dtos(double d)
{
    std::ostringstream str;
    str << d;
    return str.str();
}